x: {
  style: {
    opacity: 0.6
    fill: orange
    stroke: "#53C0D8"
    stroke-width: 5
    shadow: true
  }
}

y: {
  style: {
    opacity: 0.6
    fill: red
    3d: true
    stroke: black
  }
}

x -> y: {
  style: {
    stroke: green
    opacity: 0.5
    stroke-width: 2
    stroke-dash: 5
  }
}

# Float between 0 and 1
style.opacity: 0.6

# CSS color or hex code
style.fill: orange

# CSS color or hex code
style.stroke: "#53C0D8"

# Integer betwen 1 and 15
style.stroke-width: 5

# Integer betwen 0 and 10
style.stroke-dash: 5

# Only applicable to shapes, except ovals
# Integer betwen 0 and 20
style.border-radius: 4

# CSS color or hex code
style.font-color: red

# Only applicable to shapes
# true or false
style.shadow: true

# Only applicable to shapes
# true or false
style.multiple: true

# Only applicable to squares
# true or false
style.3d: true

# Only applicable to edges
style.animated: false

# Only applicable to shapes
# Can be an external URL
link: https://google.com
# Or an internal board
link: Overview.Untitled Board 2